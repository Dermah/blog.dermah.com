{
  "name": "blog.dermah.com",
  "version": "1.5.0",
  "description": "Static site blog src for blog.dermah.com",
  "main": "index.js",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "SERVE=true npx @11ty/eleventy --serve",
    "deploy": "aws --profile dermah s3 sync --acl public-read ./dist s3://blog.dermah.com",
    "decache": "aws --profile dermah cloudfront create-invalidation --distribution-id E1XQ0JZXRLB5H7 --paths '/*' '/index.html' '/css/index.css'",
    "lfs-server": "(cd lfs-server && ./server.sh)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dermah/blog.dermah.com.git"
  },
  "keywords": [
    "blog"
  ],
  "author": "Sputnik <inquiry@dermah.com> (https://blog.dermah.com/)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Dermah/blog.dermah.com/issues"
  },
  "homepage": "https://github.com/Dermah/blog.dermah.com#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "ansi-to-html": "^0.6.14",
    "aws-cdk": "^1.45.0",
    "markdown-it-implicit-figures": "^0.10.0",
    "node-git-lfs": "0.0.1"
  },
  "dependencies": {
    "prism-themes": "^1.4.0"
  }
}
